	aopo:has_key_event_relationship	ker:244,ker:245,ker:243,ker:279,ker:278,ker:277,ker:240,ker:182,ker:272,ker:241,ker:169,ker:242 ;
	aopo:has_molecular_initiating_event	ke:213 ;
